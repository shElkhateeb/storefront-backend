{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "Storefront API using node js connected to a Postgres database.",
  "main": "server.ts",
  "scripts": {
    "prettier": "prettier --config .prettierrc ./src/**/*.ts --write",
    "start": "tsc && node dist/server.js",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "test": "set ENV=test && tsc && db-migrate up --env test && tsc && jasmine && db-migrate reset"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/node": "^18.0.0",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.12",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.3.0",
    "prettier": "^2.6.2",
    "supertest": "^6.2.2",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.7.4"
  },
  "keywords": []
}
